{
    "name": "{{ velocity.sdk.package }}",
    "license": "proprietary",
    "type": "library",
    "autoload": {
        "psr-4": {"{{ velocity.sdk.namespace|replace({"\\": "\\\\"}) }}\\": "src/"}
    },
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "5.*",
        "symfony/config": "2.*",
        "symfony/event-dispatcher": "2.*",
        "symfony/http-foundation": "2.*",
        "symfony/http-kernel": "2.*",
        "symfony/dependency-injection": "2.*",
        "symfony/form": "2.*",
        "phppro/sdk": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "phploc/phploc": "2.*",
        "escapestudios/symfony2-coding-standard": "~2.0"
    },
    "config": {
        "bin-dir": "bin"
    }
}
