{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <img width="30" height="30" alt="Database" style="vertical-align: middle;" src="data:image/png;base64," />

        <span class="sf-toolbar-status sf-toolbar-status-green">{{ collector.queries|length }}</span>
    {% endset %}
    {% set text %}
        <div class="sf-toolbar-info-piece">
            <b>Total Queries</b> <span class="sf-toolbar-status sf-toolbar-status-green">{{ collector.queries|length }}</span><br/>
        </div>
    {% endset %}
    {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

{% block menu %}
    <span class="label">
    <span class="icon"><img width="30" height="30" alt="Database" style="vertical-align: middle;" src="data:image/png;base64," /></span>
    <strong>Database</strong>
    <span class="count">
        <span>{{ collector.queries|length }}</span>
    </span>
    </span>{% endblock %}

{% block panel %}
    {% if collector.queries is defined and collector.queries|length %}
        <h2>Database Queries Summary</h2>
        <table>
            <tr>
                <th>#</th>
                <th>Type</th>
                <th>Query</th>
                <th>DURATION</th>
            </tr>
            {% for id,query in collector.queries %}
                <tr>
                    <td><a href="#query-{{ id + 1 }}">{{ id + 1 }}</a></td>
                    <td>{{ query.type }}</td>
                    <td>{{ query.query }}</td>
                    <td>{{ (query.duration * 1000)|round(3) }} ms</td>
                </tr>
            {% endfor %}
        </table>
        <h2>Database Queries Details</h2>
        {% for id,query in collector.queries %}
            <h3><a name="query-{{ id + 1 }}">#{{ id + 1 }} : {{ query.query }}</a></h3>
            <table>
                <tr>
                    <th>Type</th>
                    <td>{{ query.type }}</td>
                </tr>
                <tr>
                    <th>Query</th>
                    <td>{{ query.query }}</td>
                </tr>
                <tr>
                    <th>Params</th>
                    <td><pre>{{ query.params|json_encode(constant('JSON_PRETTY_PRINT')) }}</pre></td>
                </tr>
                <tr>
                    <th>Result</th>
                    <td><pre>{{ query.result|json_encode(constant('JSON_PRETTY_PRINT')) }}</pre></td>
                </tr>
                <tr>
                    <th>Duration</th>
                    <td>{{ (query.duration * 1000)|round(3) }} ms</td>
                </tr>
            </table>
        {% endfor %}
    {% else %}
        <h2><i>No Database Queries detected.</i></h2>
    {% endif %}
{% endblock %}